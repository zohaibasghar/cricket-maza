{
  "expo": {
    "name": "cricket-maza",
    "slug": "cricket-maza",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "privacy": "public",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5E41E6"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5E41E6"
      },
      "package": "com.meharzohaib123.cricketmaza"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {},
    "plugins": ["expo-font"]
  }
}
